<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  <template class="GaussianPoissonPage" parent="GaussianPage">
    <property name="title" translatable="yes">Poisson</property>
    <property name="description" translatable="yes">Probability distribution of x events ocurring in a interval of time or space, with constant mean rate.</property>
    <property name="tag">poisson</property>
    <property name="variable-type">discrete</property>
    <child>
      <object class="AdwPreferencesGroup">
        <child>
          <object class="AdwSpinRow" id="mean_row">
            <property name="title">Mean</property>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="lower">0</property>
                <property name="upper">100000</property>
                <property name="value">0</property>
                <property name="page-increment">10</property>
                <property name="step-increment">1</property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
